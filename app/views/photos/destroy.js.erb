$('#photos').html("<%= j render 'photos_list' %>")
<% if @craft.photos.blank? %>
  $('#photo_check').hide();
<% end %>

<% if !@craft.active && !@craft.price.blank? && !@craft.listing_name.blank? && !@craft.photos.blank? && !@craft.address.blank? %>
    $('#publish_button').attr('disabled', false);
<% else %>
    $('#publish_button').attr('disabled', true);
<% end %>
