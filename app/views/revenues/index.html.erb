<div class="row">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading">収益</div>
      <div class="panel-body">
        <div class="container text-center">
          <% if @reservations.blank? %>
              <h3>今週は予約はありません</h3><br/>
              <%= link_to "作品をシェアしましょう", new_craft_path, class: "btn btn-normal" %>
          <% else %>
              <%= column_chart @this_week_revenue, colors: ["#00A699"] %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
